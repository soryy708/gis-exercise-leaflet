        "copy-webpack-plugin": "^8.1.1",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.2",
        "eslint": "^7.24.0",
        "eslint-plugin-compat": "^3.9.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-react": "^7.23.2",
        "file-loader": "^6.2.0",
        "node-sass": "^5.0.0",
        "postcss": "^8.2.10",
        "postcss-loader": "^5.2.0",
